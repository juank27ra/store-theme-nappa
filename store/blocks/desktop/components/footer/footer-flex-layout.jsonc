    "title": "row industry and commerce",
    "children": [
      "image#desktop__image--industry-commerce"
    ],
    "props": {
      "blockClass": "desktop__industry--commerce"
    }
  },
  "image#desktop__image--industry-commerce": {
    "title": "image of industry and commerce",
    "props": {
      "src": "assets/img/Home/footer/footer__camara--comiercio.png",
      "title": "industry and commerce",
      "blockClass": "desktop__image--industry-commerce"
    }
  },

  "flex-layout.row#desktop__industry--commerce--text": {
    "title": "text content industry and commerce",
    "children": ["rich-text#desktop__text--industry&commerce"],
    "props": {
      "blockClass": "desktop__industry--commerce--text"
    }
