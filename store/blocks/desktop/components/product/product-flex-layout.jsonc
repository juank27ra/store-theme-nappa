  "flex-layout.row#desktop__pdp--care": {
    "title": "flex pdp care",
    "children": [
      "menu#menu__care--main"
    ],
    "props": {
      "blockClass": "desktop__pdp--care"
    }
  },
  "flex-layout.row#desktop__pdp--specifications": {
    "title": "flex pdp specifications",
    "children": [
      "menu#menu__specifications--main"
