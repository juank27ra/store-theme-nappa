{
  "flex-layout.col#desktop__product--image": {
    "title": "col left global product image",
    "children": [
      "product-images"
    ],
    "props": {
      "blockClass": "desktop__product--image"
    }
  },
  "flex-layout.col#desktop__product--data": {
    "title": "col right global product data",
    "children": [
      "vtex.store-components:product-name",
      "product-identifier.product",
      "product-price",
      "rich-text#desktop__pdp--quoter",
      "product-separator",
      "product-quantity",
      "sku-selector",
      "flex-layout.row#desktop__pdp--buy-button",
      // "orderquote",
      "disclosure-layout#desktop__disclosure--credit-simulation",                //
      "rich-text#desktop__pdp--also-available",
      "flex-layout.row#desktop__pdp--specifications",
      "flex-layout.row#desktop__pdp--description",
      "flex-layout.row#desktop__pdp--shipping-policies",
      "flex-layout.row#desktop__pdp--care",
      "share"
