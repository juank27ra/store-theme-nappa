    "props": {
      "orientation": "vertical"
    }
  },
  "menu-item#about-us": {
    "title": "menu about-us",
    "props": {
      "id": "menu-about-us",
      "type": "custom",
      "itemProps": {
        "type": "internal",
        "href": "/",
        "tagTitle": "Acerca de nosotros",
